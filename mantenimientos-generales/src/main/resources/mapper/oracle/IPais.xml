<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="ob.unibanca.sicf.mantenimientosgenerales.mapper.IATMRedAsociadaMapper">

       
     <sql id="consultaVistaMant">
       SELECT  
       	id_membresia
		,id_giro_negocio
		,id_categoria_negocio
		,descripcion

       FROM vie_man_pais
    </sql>

       
     <select id="buscarTodos" resultType="Pais"
            statementType="PREPARED">
        <include refid="consultaVistaMant"/>
    </select>
</mapper>

<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="ob.unibanca.sicf.reportes.mapper.ITablaMapper">


    <sql id="consultaVistaMant">
       SELECT 
		  ID_TABLA      AS ID_TABLA,
		  NOMBRE        AS NOMBRE,
		  DESCRIPCION   AS DESCRIPCION,
		  DESC_CORTA    AS DESC_CORTA,
		  ALIAS         AS ALIAS,
		  ID_TIPO_TABLA AS ID_TIPO_TABLA
		FROM VIE_MAN_TABLA
    </sql>

     <select id="buscarTodos" resultType="Tabla"
            statementType="PREPARED">
        <include refid="consultaVistaMant"/>
    </select>
    
</mapper>

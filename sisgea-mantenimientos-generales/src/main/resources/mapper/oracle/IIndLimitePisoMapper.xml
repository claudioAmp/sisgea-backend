<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="edu.taller.sisgea.mantenimientosgenerales.mapper.IIndLimitePisoMapper">

    <sql id="consultaVistaMant">
      SELECT 
	    id_membresia,
	    descripcion_membresia,
	    id_limite_piso,
	    descripcion
	  FROM VIE_MAN_IND_LIMITE_PISO
    </sql>

    <select id="buscarTodos" resultType="IndLimitePiso" statementType="PREPARED">
       <include refid="consultaVistaMant" />
    </select>

</mapper>
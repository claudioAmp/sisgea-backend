<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="edu.taller.sisgea.mantenimientosgenerales.mapper.IParametroConsultaMapper">

	<sql id="consultaVistaMant">
		SELECT id_parametro
        	 , max_dias_consulta	
        FROM VIE_MAN_PARAMETRO_CONSULTA
	</sql>
	
    <select id="buscarTodos" resultType="ParametroConsulta" statementType="PREPARED">
        <include refid="consultaVistaMant" />
    </select>
    
</mapper>

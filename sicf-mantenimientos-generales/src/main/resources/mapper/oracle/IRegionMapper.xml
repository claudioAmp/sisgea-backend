<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="ob.unibanca.sicf.mantenimientosgenerales.mapper.IRegionMapper">

    <sql id="consultaVistaMant">
       SELECT  
       	 ID_MEMBRESIA
		,DESC_MEMBRESIA
		,ID_REGION
		,DESC_REGION
       FROM VIE_MAN_REGION
    </sql>
    
    <sql id="orden">
    	ORDER BY ID_MEMBRESIA ASC
    </sql>

   	<select id="buscarTodos" resultType="Region"
            statementType="PREPARED">
        <include refid="consultaVistaMant"/>
        <include refid="orden"/>
    </select>
    
</mapper>

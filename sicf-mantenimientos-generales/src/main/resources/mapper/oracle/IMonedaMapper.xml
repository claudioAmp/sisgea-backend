<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="ob.unibanca.sicf.mantenimientosgenerales.mapper.IMonedaMapper">

    <select id="buscarTodos" resultType="Moneda" statementType="PREPARED">
        SELECT id_moneda
             , descripcion      AS descripcion_moneda
             , abreviatura
        FROM MAE_MONEDA
        ORDER BY id_moneda ASC
    </select>

</mapper>

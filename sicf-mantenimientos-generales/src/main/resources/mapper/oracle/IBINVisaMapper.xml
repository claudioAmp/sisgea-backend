<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="ob.unibanca.sicf.mantenimientosgenerales.mapper.IBINVisaMapper">

    <sql id="consultaVistaMant">
        SELECT idBin
	           , binProcesador
	           , region 
	           , pais
	           , tipoBin
	           , participationFlag
	           , visaDirect
	           , binTifBypassFlag 
	           , multinationalMerchantAccept
	           , domesticOctPif
	           , crossBorderOctPif
	           , octGeoBlocking
	           , testIndicator
	           , oboIndicator
            FROM VIE_MAN_BIN_VISA
    </sql>
    
    <sql id="orden">
    	ORDER BY idBin ASC
    </sql>

    <select id="buscarTodos" resultType="BINVisa" statementType="PREPARED">
       <include refid="consultaVistaMant" />
       <include refid="orden"/>
    </select>
</mapper>
<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="ob.unibanca.sicf.consultasgenerales.mapper.ITipoCambioSBSMapper">
		
	<sql id="consulta">
		SELECT *
        FROM MOV_TIPO_CAMBIO_SBS
	</sql>
	
	<sql id="orden">
		ORDER BY fecha_tipo_cambio DESC
	</sql>
	
	<select id="buscarTodos" resultType="TipoCambioSBS" statementType="PREPARED">
        <include refid="consulta"/>
        <include refid="orden"/>
    </select>
    
    <select id="buscarIdTipoCambio" resultType="TipoCambioSBS" statementType="PREPARED">
        <include refid="consulta"/>
        <where>
        	<if test="fechaTipoCambio != null">
        		fecha_tipo_cambio = #{fechaTipoCambio}
        	</if>
        </where>
    </select>
    
    <select id="mantener" parameterType="ParametroMantenimiento"
            statementType="CALLABLE">
        {
            CALL PKG_CONSULTAS.PRC_CON_TIPO_CAMBIO_SBS(
                #{modo,                                 jdbcType=VARCHAR, mode=IN},
                #{objetoMantenido.tipoCambio, 			jdbcType=NUMERIC, mode=IN},
                #{objetoMantenido.fechaTipoCambio, 		jdbcType=DATE, mode=IN},
                #{usuario,                              jdbcType=VARCHAR, mode=IN}
            )
        }
    </select>

</mapper>